<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<TabPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.repStageController">
  <tabs>
    <Tab text="Items Report">
         <content>
            <BorderPane prefHeight="200.0" prefWidth="200.0">
               <center>
                  <TableView fx:id="itemReport" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <columns>
                      <TableColumn fx:id="piddesc" prefWidth="150.0" text="Product Description" />
                      <TableColumn fx:id="pid" minWidth="0.0" prefWidth="171.0" text="Product ID" />
                    </columns>
                  </TableView>
               </center></BorderPane>
         </content>
    </Tab>
    <Tab text="RF Flow Diagram">
         <content>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" />
         </content>
    </Tab>
  </tabs>
</TabPane>
